@{
    Layout = null;
}

<!DOCTYPE html>
<html>
<head>
    <meta http-equiv="Content-Type" content="text/html; charset=utf-8"/>
    <title>账户管理</title>
    <link href="../../Content/Css/bootstrap.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/Css/bootstrap-theme.css" rel="stylesheet" type="text/css" />
    <link href="../../Content/Css/public.css" rel="stylesheet" type="text/css" />
    <script src="../../Scripts/jquery-1.8.3.min.js" type="text/javascript"></script>
    <script type="text/javascript">
        window.ControlIFrame = function (mShow) {
            if (mShow) {
                $('#iframe2').hide();
                $('#iframe1').show();
            } else {
                $('#iframe1').hide();
                $('#iframe2').show();
            }
        };
    </script>
    <style>
        .manage_r1 {
            width: 100%;
            display:none;
            min-height: 98%;
            border:0px;
            background: #f6f6f6;
        }
        .manage_r 
        {
            border:0px;
        width: 100%;
        min-height: 100%;
        background: #f6f6f6;
        }
    </style>

</head>
<body style=" background: #f6f6f6;border:0px">
  
        <iframe id="iframe1" src="/@ViewData["controller"]/Index1"  class="manage_r"></iframe>
        <iframe id="iframe2" src="/@ViewData["controller"]/Index2" class="manage_r1"></iframe>
</body>
</html>
